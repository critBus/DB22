	<queryString>
		<![CDATA[SELECT
     libro."edicion" AS libro_edicion,
     titulo."nombre" AS titulo_nombre,
     titulo."fecha" AS titulo_fecha,
     autor."nombre" AS autor_nombre,
     disponibilidad."ubicacion" AS disponibilidad_ubicacion
FROM
     "public"."titulo" titulo INNER JOIN "public"."libro" libro ON titulo."id" = libro."tituloid"
     INNER JOIN "public"."autor_titulo" autor_titulo ON titulo."id" = autor_titulo."tituloid"
     INNER JOIN "public"."disponibilidad" disponibilidad ON titulo."id" = disponibilidad."tituloid"
     INNER JOIN "public"."autor" autor ON autor_titulo."autorid" = autor."id"
WHERE
    disponibilidad."id"=(SELECT MIN(disponibilidad."id") FROM 
   "public"."disponibilidad" disponibilidad
   WHERE disponibilidad."tituloid"=titulo."id")
   AND
   autor_titulo."autorid"=(SELECT MIN( autor_titulo."autorid") FROM 
   "public"."autor_titulo" autor_titulo
   WHERE autor_titulo."tituloid"=titulo."id")
]]>
	</queryString>
